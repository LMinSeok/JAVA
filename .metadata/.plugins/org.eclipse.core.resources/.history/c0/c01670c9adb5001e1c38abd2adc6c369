package ex01;

public class Hello {

	public static void main(String[] args) {
		
		System.out.println("Hello World"); //ctrl + d > 줄삭제
		System.out.println(""); //한줄, alt + 상하방향키 > 줄이동
		System.out.println("------------"); //ctrl + alt + 아래방향키 > 줄복사
		
		int SCORE = 20;
		final int SCORE = 30; //final 상수화(대문자), 변경 불가
		
		
		System.out.println("SCORE : " + SCORE);
		System.out.println(SCORE);
	}

}
